{
    "name": "xinningsu/yii2-raygun",
    "description": "Raygun Yii2 logger, Raygun integration for Yii2.",
    "keywords": [
        "yii2",
        "raygun",
        "raygun4php",
        "raygun yii2 logger",
        "error monitoring",
        "error reporting",
        "error tracking"
    ],
    "homepage": "https://github.com/xinningsu/yii2-raygun",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Su",
            "email": "deng5765@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": "^2.0.0",
        "mindscape/raygun4php": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sulao\\YiiRaygun\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
